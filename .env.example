# Dashboard Application Environment Variables
# Copy this file to .env and update the values as needed

# API Configuration
VITE_API_BASE_URL=https://api.dashboard.example.com
VITE_APP_VERSION=1.0.0

# Feature Flags
VITE_ENABLE_REALTIME=true
VITE_ENABLE_EXPORT=true
VITE_ENABLE_NOTIFICATIONS=true
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_DARK_MODE=true

# Development Settings
NODE_ENV=development
VITE_DEBUG_MODE=true

# Authentication (when implemented)
VITE_AUTH_PROVIDER=jwt
VITE_AUTH_DOMAIN=auth.dashboard.example.com

# Analytics (optional)
VITE_GOOGLE_ANALYTICS_ID=GA-XXXXXXXXX
VITE_MIXPANEL_TOKEN=your-mixpanel-token

# Error Tracking (optional)
VITE_SENTRY_DSN=https://your-sentry-dsn@sentry.io/project-id

# Performance Monitoring
VITE_PERFORMANCE_MONITORING=true
VITE_PERFORMANCE_SAMPLE_RATE=0.1

# Deployment Configuration
VERCEL_TOKEN=your-vercel-token
VERCEL_ORG_ID=your-vercel-org-id
VERCEL_PROJECT_ID=your-vercel-project-id

# Database (if backend is implemented)
DATABASE_URL=postgresql://user:password@localhost:5432/dashboard
REDIS_URL=redis://localhost:6379

# Email Service (if notifications are implemented)
SMTP_HOST=smtp.example.com
SMTP_PORT=587
SMTP_USER=your-email@example.com
SMTP_PASS=your-email-password

# File Storage (if file uploads are implemented)
AWS_ACCESS_KEY_ID=your-aws-access-key
AWS_SECRET_ACCESS_KEY=your-aws-secret-key
AWS_REGION=us-east-1
AWS_S3_BUCKET=dashboard-uploads

# Webhook Configuration
WEBHOOK_SECRET=your-webhook-secret-key

# Rate Limiting
RATE_LIMIT_WINDOW_MS=3600000
RATE_LIMIT_MAX_REQUESTS=1000

# Security
JWT_SECRET=your-jwt-secret-key
ENCRYPTION_KEY=your-encryption-key
CORS_ORIGIN=https://dashboard.example.com

# Monitoring and Logging
LOG_LEVEL=info
ENABLE_REQUEST_LOGGING=true
HEALTH_CHECK_INTERVAL=30000
